    <div class="container">
      {{#each articles}}
        <div class="row" id="{{_id}}">
          <div class="col s12 m12">
            <div class="card #212121 grey darken-4  hoverable">
              <div class="card-content white-text">
                <span class="card-title">{{title}}</span>
                <p>{{summary}}</p>
              </div>
              <div class="card-action">
                <a class="btn black-text waves-effect  #ffeb3b yellow btn #000000 black-text card-button" target="_blank" href="https://screenrant.com{{URL}}">Read Article</a>
                <a data-id="{{_id}}" id="save-article" class="btn waves-effect  #ffeb3b yellow btn #000000 black-text right card-button" href="#">Save this Article</a>
              </div>
            </div>
          </div>
        </div>
      {{/each}}
    </div>
  